<source>
  type tail
  format /^(?<host>[^ ]*) [^ ]* [^ ]* \[(?<time>[^\]]*)\] "(?<method>\S+) +(?<path>[^ ]+) +\S*" (?<code>[^ ]*) (?<size>[^ ]*) "(?<referer>[^\"]*)" "(?<agent>[^\"]*)" "[^\"]*" "[^\"]*" "[^\"]*" (?<restime>[^ ]*) "(?<vckey>[^\"]*)" "(?<vckeylg>[^\"]*)" "(?<carrier>[^\"]*)" "(?<redirect_url>[^\"]*)" "(?<basehash>[^\"]*)" "(?<taglist>[^\"]*)" "(?<tag>[^\"]*)"$/
  time_format %d/%b/%Y:%H:%M:%S %z
  path /var/log/httpd/<%= name %>-access_log.link
  tag forward.apache.access.<%= name.split(".")[0] %>
  pos_file /var/log/td-agent/<%= name %>-access_log.pos
</source>
